\name{DEoptimLoop} 
\alias{DEoptimLoop} 
\title{likeLTD::DEoptimLoop} 
\usage{DEoptimLoop(PARAMS)}
\arguments{
  \item{PARAMS}{Parameters for optimization as generated by optimization.params}
}
\value{List.} 
\description{Values returned by DEoptim after optimization. See DEoptim help page for more details.}
\details{
  Optimize over parameter space, while checking for converence every 50 iterations.
}
\seealso{DEoptim}
